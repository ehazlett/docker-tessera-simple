    "queries": {
      "allContainerMemUsage": {
        "name": "allContainerMemUsage",
        "targets": [
          "alias(sum(transformNull(docker.stats.containers.nodes.*.*.memory.usage, 0)), \"total memory\")"
        ]
      },
      "allContainerRx": {
        "name": "allContainerRx",
        "targets": [
          "sum(docker.stats.containers.nodes.*.*.network.rxBytes)"
        ]
      },
      "allContainerTx": {
        "name": "allContainerTx",
        "targets": [
          "sum(docker.stats.containers.nodes.*.*.network.txBytes)"
        ]
      },
      "clusterEvents": {
        "name": "clusterEvents",
        "targets": [
          "alias(transformNull(docker.stats.cluster.events, 0), \"event\")"
        ]
      },
      "cpuUsageDiff": {
        "name": "cpuUsageDiff",
        "targets": [
          "aliasByNode(asPercent(docker.stats.containers.nodes.*.*.cpu.totalUsage), 5)"
        ]
      },
      "memUsageDiff": {
        "name": "memUsageDiff",
        "targets": [
          "aliasByNode(asPercent(docker.stats.containers.nodes.*.*.memory.usage), 5)"
        ]
      },
      "totalNetRxTx": {
        "name": "totalNetRxTx",
        "targets": [
          "group(alias(averageSeries(transformNull(docker.stats.containers.nodes.*.*.network.rxBytes, 0)), \"total net (rx)\"),alias(averageSeries(transformNull(docker.stats.containers.nodes.*.*.network.txBytes, 0)), \"total net (tx)\"))"
        ]
      }
    }
  }
